{
  "compilerOptions": {
      "outDir": "./dist/",
      "allowSyntheticDefaultImports": true,
      "noImplicitAny": true,
      "moduleResolution": "node",
      "baseUrl": "./",
      "sourceMap": true,
      "module": "commonjs",
      "target": "es5",
      "jsx": "react",
      "allowJs": true,
      "noImplicitThis": true,
      "strictNullChecks": true,
      "lib": ["es6", "dom", "es2019"],
  }
}